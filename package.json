{
  "name": "macgarden",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "bunx biome lint src/",
    "check-formatting": "bunx biome format src/",
    "format": "bunx biome format --write src/",
    "cypress:open": "cypress open",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "bun run pages:build && wrangler pages dev",
    "deploy": "bun run pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "test:e2e": "start-server-and-test dev http://localhost:3000 'cypress run'",
    "test:coverage": "nyc report --reporter=text-summary"
  },
  "dependencies": {
    "@babel/preset-react": "^7.26.3",
    "@radix-ui/react-dialog": "^1.1.4",
